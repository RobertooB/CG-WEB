                                                <input id="Delete" type="reset" value="Borrar" AutoPostBack="False" Style="color: black; width: 80px; height: 30px; float: left; margin-left: 28px; background: white; border-radius: 15px; box-shadow: 0 0 6px white;" />
                                            </dx:LayoutItemNestedControlContainer>
                                        </LayoutItemNestedControlCollection>
